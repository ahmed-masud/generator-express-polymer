{
   "name": "<%= appname %>"
  , "private": true
  , "generator-settings": {
       "appname": "<%= appname %>"
     , "title": "<%= title %>"
     , "version": "<%= version %>"
     , "generator": "<%= generator %>"
     , "license": "<%= license %>"
     , "viewEngine": "<%= viewEngine %>"
     , "starterKit": "<%= starterKit %>"
     , "serverPort": <%= serverPort %>
     , "devProxyPort": <%= devProxyPort %>
     , "addPassport": <%= addPassport %>
     , "skipInstall": <%= skipInstall %>
     , "viewEngine": "<%= viewEngine %>"
     , "middleWare": "<%= middleWare %>"
     , "backEnd": "<%= backEnd %>"
  }
  , "dependencies": {
      "serve-favicon": "^2.3.0",
      "body-parser": "^1.13.0",
      "morgan": "^1.6.0",
      "debug": "^2.0.0",
      "cookie-parser": "^1.3.5"<% if (middleWare =="Express") { %>,
      "express": "^4.0.0"<% } else if(middleWare == "HAPI") { %>,
      "hapi": "^8.8.0"<% } else if (middleWare == "Koa") { %>,
      "koa": "^0.21.0"<% } %><% if (viewEngine == "html") { %>,
      "ehp": ">=1.0.0"
      <% } %>
  }

  , "devDependencies": {
      "browser-sync": "^2.7.7"
      , "deprecated": "*"
      , "del": "^1.1.1"
      , "glob": "^5.0.6"
      , "gulp": "^3.8.5"
      , "gulp-autoprefixer": "^2.1.0"
      , "gulp-cache": "^0.2.8"
      , "gulp-util": "^3.0.6"
      , "gulp-changed": "^1.0.0"
      , "gulp-cssmin": "^0.1.6"
      , "gulp-flatten": "^0.0.4"
      , "gulp-if": "^1.2.1"
      , "gulp-imagemin": "^2.2.1"
      , "gulp-jshint": "^1.6.3"
      , "gulp-load-plugins": "^0.10.0"
      , "gulp-minify-html": "^1.0.2"
      , "gulp-rename": "^1.2.0"
      , "gulp-replace": "^0.5.3"
      , "gulp-size": "^1.0.0"
      , "gulp-uglify": "^1.2.0"
      , "gulp-uncss": "^1.0.1"
      , "gulp-useref": "^1.1.2"
      , "gulp-vulcanize": "^6.0.0"
      , "gulp-nodemon": "^2.0.3"
      , "jshint-stylish": "^2.0.0"
      , "merge-stream": "^0.1.7"
      , "opn": "^2.0.0"
      , "require-dir": "^0.3.0"
      , "run-sequence": "^1.0.2"
      , "vulcanize": ">= 1.4.2"
      , "web-component-tester": "^3.1.3"
  }

  , "engines": {
      "node": ">=0.10.0"
      <% if (viewEngine == "html") { %>
      , "ehp": ">=1.0.0"
      <% } %>
  }

  , "scripts" : {
       "start": "gulp serve"
      , "test": "gulp test:local"
  }

}
